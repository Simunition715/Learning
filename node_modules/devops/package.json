      "process": [],
      "tokens": {},
      "answers": {
        "repoUrl": "${{repoUrl}}",
        "repoType": "${{repoType}}",
        "masterBranch": "${{masterBranch}}",
        "figmaUrl": "${{figmaUrl}}"
      }
    }
  },
  "skeleton": {
    "process": [],
    "tokens": {
      "figmaUrl": {
        "description": "Enter the URL to the figma project this appliction is associated with:"
      },
      "repoUrl": {
